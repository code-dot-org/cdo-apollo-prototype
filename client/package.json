{
  "name": "cdo-apollo-client",
  "version": "0.1.0",
  "private": false,
  "main": "./lib/cjs/src/index.js",
  "module": "./lib/esm/src/index.js",
  "types": "./lib/esm/src/index.d.ts",
  "babel": {
    "presets": [
      "@babel/preset-react"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.3.21",
    "@reach/router": "^1.2.1",
    "@types/node": "^12.12.14",
    "@types/reach__router": "^1.2.6",
    "@types/react": "^16.9.15",
    "@types/react-dom": "^16.9.4",
    "emotion": "^9.2.12",
    "graphql": "^15.5.0",
    "polished": "^3.4.1",
    "react-emotion": "^9.2.12",
    "react-redux": "^7.2.4",
    "react-scripts": "^3.4.1",
    "typescript": "^3.7.3"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "firefox": "BROWSER=firefox react-scripts start",
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs",
    "publish:npm": "rm -rf dist && mkdir dist &&  babel src -d dist --copy-files",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "run:ios-demo": "npx artillery run apollo-internal-demos/ios-workload.yml",
    "codegen": "apollo client:codegen --target typescript --watch"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/plugin-syntax-class-properties": "^7.12.13",
    "@babel/preset-react": "^7.14.5",
    "@testing-library/jest-dom": "^4.0.0",
    "@testing-library/react": "^8.0.7",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^24.0.23",
    "@types/react-redux": "^7.1.18",
    "apollo": "^2.33.4",
    "artillery": "^1.6.0-26",
    "npm-watch": "^0.6.0",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "redux": "^4.1.0"
  }
}
